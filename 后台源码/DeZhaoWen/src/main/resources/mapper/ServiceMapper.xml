<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.accp.mapper.ServiceMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.accp.domain.Service">
        <id column="ser_id" property="serId" />
        <result column="ser_name" property="serName" />
        <result column="ser_parent" property="serParent" />
        <result column="ser_img" property="serImg" />
        <result column="ser_tag" property="serTag" />
        <result column="reserved1" property="reserved1" />
        <result column="reserved2" property="reserved2" />
        <result column="reserved3" property="reserved3" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ser_id, ser_name, ser_parent, ser_img, ser_tag, reserved1, reserved2, reserved3
    </sql>

</mapper>
